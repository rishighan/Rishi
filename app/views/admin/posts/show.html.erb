<p id="notice"><%= notice %></p>

<p>
  <b>Title:</b>
  <%= @post.title %>
</p>

<p>
  <b>Category:</b>
  <%= @post.category_id %>
</p>

<p>
  <b>Content:</b>
  <%= @post.content.html_safe %>
</p>

<p id="attachments">

	<% @post.attachments.each do |at|%>
	<%= image_tag at.photo.url(:thumb) %>
	<%end%>
</p>

<%= link_to 'Edit', edit_admin_post_path(@post) %> |
<%= link_to 'Back', admin_posts_path %>
