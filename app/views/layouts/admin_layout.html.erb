<!DOCTYPE html>
<html>
<head>
  <title>Rishi</title>
  <%= stylesheet_link_tag  "application" %>
 
  <%= javascript_include_tag "application", :cache => false %>
  <%= javascript_include_tag :vendor, :cache => false %>

  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>



<div class="container">
<div class="navbar">
  <div class="navbar-inner">
    <div class="container">
      <ul class="nav">
        <li><%= link_to "Dashboard" ,admin_url%> </li>
        <li><%= link_to  "Posts", admin_posts_url%> </li>
        <li><%= link_to  "Store", admin_products_url%> </li>
      </ul>
    </div>
  </div>
</div>

<p class="alert alert-info">
	<%= notice %>
	<span class="current-user">
	<% if user_signed_in? %>
	Signed in as <%=current_user.email %>  <%= link_to('Logout', destroy_user_session_path, :method => :delete, :class=>"btn btn-mini" )%> 
	<%end%>
</span>
</p>

<p class="alert alert-error"><%= alert %></p>



<%= yield %>

</div>

</body>
</html>
